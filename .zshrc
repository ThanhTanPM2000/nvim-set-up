source ~/.config/zsh/theme.zsh
source ~/.config/zsh/base.zsh
source ~/.config/zsh/golang.zsh
source ~/.config/zsh/homebrew.zsh
source ~/.config/zsh/java.zsh
source ~/.config/zsh/bun.zsh
source ~/.config/zsh/anaconda.zsh

# Which plugins would you like to load?
# Standard plugins can be found in $ZSH/plugins/
# Custom plugins may be added to $ZSH_CUSTOM/plugins/
# Example format: plugins=(rails git textmate ruby lighthouse)
# Add wisely, as too many plugins slow down shell startup.
source ~/.config/zsh/plugins/init.zsh # don't delete this find
source ~/.config/zsh/plugins/vi-mode.zsh
source ~/.config/zsh/plugins/nvm.zsh
source ~/.config/zsh/plugins/pyenv.zsh
source ~/.config/zsh/plugins/zsh-autosuggestions.zsh
source ~/.config/zsh/plugins/zsh-syntax-highlighting.zsh
source ~/.config/zsh/plugins/eza.zsh
source ~/.config/zsh/plugins/zoxide.zsh
source $ZSH/oh-my-zsh.sh
